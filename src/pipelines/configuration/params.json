{
    "models": [
        "logistic_regression",
        "sgd_classifier",
        "random_forest",
        "xgboost",
        "lightgbm"
    ],
    "training_params": {
        "data_sampling": "rose",
        "rose_shrinkage": 0.5,
        "rose_upsampled_minority_proportion": 0.3
    },
    "models_params": {
        "logistic_regression": {
            "max_iter": 500
        },
        "sgd_classifier": {
            "loss": "modified_huber"
        },
        "xgboost": {
            "eval_metric": "logloss",
            "scale_pos_weight": 10,
            "verbosity": 0
        },
        "lightgbm": {
            "verbosity": 0,
            "is_unbalance": true
        }
    },
    "fit_params": {
        "xgboost": {
            "verbose": 0
        }
    },
    "fraud_delay_days": 7,
    "features": [
        "amount", "has_chip", "gender", "online_transaction", "amex", "discover",
